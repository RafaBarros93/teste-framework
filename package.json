{
  "name": "teste-framework",
  "version": "1.0.0",
  "main": "index.ts",
  "author": "rafaeldevfina <rafael@finafacil.com>",
  "license": "MIT",
  "scripts": {
    "start": "nodemon src/main/index.js --ignore __tests__",
    "test": "jest --colors --noStackTrace --passWithNoTests --runInBand",
    "test:unit": "yarn test -- --watch -c jest-unit-config.js",
    "test:integration": "yarn test - --watch -c jest-integration-config.js",
    "test:staged": "yarn test -- --findRelatedTests --silent",
    "swagger-autogen": "node src/main/swagger.js "
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "devDependencies": {
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.4",
    "nodemon": "^2.0.15",
    "standard": "^16.0.4"
  },
  "dependencies": {
    "express": "^4.17.2",
    "fast-glob": "^3.2.11",
    "morgan": "^1.10.0",
    "supertest": "^6.2.2",
    "swagger-autogen": "^2.18.8",
    "swagger-ui-express": "^4.3.0"
  }
}
